package programmers;

import java.util.Arrays;
import java.util.Comparator;

public class programmers17686 {

	public static void main(String[] args) {
		String[] files = {"img12.png", "img10.png", "img02.png", "img1.png", "IMG01.GIF", "img2.JPG"};
		solution(files);
	}
	
	public static String[] solution(String[] files) {
		String[] answer = {};
		/*String[] head = new String[files.length];
		String[] number = new String[files.length];
		String[] tail = new String[files.length];*/
		
		Comparator<String> myComparator = new Comparator<String>() {

			@Override
			public int compare(String o1, String o2) {
				String Head1 = (o1.split("[0-9]")[0]).toLowerCase();
				String Head2 = (o2.split("[0-9]")[0]).toLowerCase();
				System.out.println(Head1+"--------------"+Head2);
				if(Head1.compareTo(Head2) > 0) {
					return 1;
				}
				return 0;
			}
		};
		
		Arrays.sort(files, myComparator);
		for (int i = 0; i < files.length; i++) {
			System.out.println(files[i]);
		}
		//String array = files[0].split("[0-9]")[0];
		//System.out.println(array);
        return answer;
    }

}
